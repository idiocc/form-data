{
  "name": "@multipart/form-data",
  "version": "0.0.0-pre",
  "description": "Multipart/Form-Data And File Upload Middleware For Koa Written In ES6 And Optimised With JavaScript Compiler.",
  "main": "depack/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "depack": "depack-dev src/depack -o depack -a -s -l -p --externs types/externs.js --hide_warnings_for node_modules/events --hide_warnings_for node_modules/http",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "b": "alamode src -o build -s",
    "d": "yarn-s d1 externs",
    "d1": "typal depack/index.js src/index.js types/index.js -c -t types/index.xml",
    "externs": "typal types/externs.js",
    "build": "yarn-s d b doc",
    "e": "alanode"
  },
  "files": [
    "depack",
    "src",
    "types/externs.js"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/idiocc/multer.git"
  },
  "keywords": [
    "multer",
    "goa",
    "koa",
    "koa2",
    "multipart",
    "form-data",
    "form",
    "middleware",
    "file",
    "upload"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idiocc/multer/issues"
  },
  "homepage": "https://github.com/idiocc/multer#readme",
  "devDependencies": {
    "@contexts/http": "^1.5.0",
    "@externs/goa": "^0.1.0",
    "@goa/busboy": "^1.1.1",
    "@goa/koa": "^1.1.3",
    "@idio/dicer": "^1.0.2",
    "@multipart/append-field": "^1.0.1",
    "@multipart/test-form-data": "^1.0.0",
    "@typedefs/goa": "^0.0.2",
    "@wrote/ensure-path": "^1.1.0",
    "@wrote/read": "^1.0.4",
    "@wrote/rm": "^1.1.5",
    "alamode": "^2.3.4",
    "catchment": "^3.3.0",
    "documentary": "^1.27.4",
    "eslint-config-artdeco": "1.0.1",
    "temp-context": "^2.1.3",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.1-alpha"
  },
  "dependencies": {
    "text-decoding": "^1.0.0"
  }
}
